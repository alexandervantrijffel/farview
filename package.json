    "@types/node": "15.12.2",
    "@types/session-file-store": "1.2.1",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "googleapis": "76.0.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "prettier": "2.3.1",
    "prettier-standard": "16.4.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node-dev": "1.1.6",
    "tslint": "6.1.3",
    "typescript": "4.3.2"
  }
