{
  "name": "farview",
  "version": "0.0.1",
  "description": "",
  "private": false,
  "scripts": {
    "comment": ".js is omitted as this is continuously updated",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "DEBUG=express:* ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "start:watch": "nodemon --ext 'ts,tsx,json' --watch '**' --exec 'DEBUG=express:* ts-node src/index.ts'",
    "start:debug": "nodemon --inspect 'src/index.ts'",
    "postinstall": "husky install",
    "prebuild": "rimraf build/",
    "build": "tsc --project ./",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --verbose false --coverage --notify"
  },
  "dependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
