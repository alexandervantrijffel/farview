{
  "name": "farview",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "dotenv": "10.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "googleapis": "76.0.0",
    "husky": "6.0.0",
    "prettier": "2.3.1",
    "prettier-standard": "16.4.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node-dev": "1.1.6",
    "tslint": "6.1.3",
    "typescript": "4.3.2"
  },
  "private": false,
  "scripts": {
    "comment": ".js is omitted as this is continuously updated",
    "start": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "start:watch": "nodemon --ext 'ts,tsx,json' --watch '**' --exec 'ts-node src/index.ts'",
    "start:debug": "nodemon --inspect 'src/index.ts'",
    "postinstall": "husky install",
    "prebuild": "rimraf build/",
    "build": "tsc --project ./",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --verbose false --coverage --notify"
  }
}
