    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "googlephotos": "0.3.4",
    "helmet": "4.6.0",
    "json-colorizer": "2.2.2",
    "jsonwebtoken": "8.5.1",
    "querystring": "0.2.1",
    "session-file-store": "1.5.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
