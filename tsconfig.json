{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "./",
    "outDir": "build",
    "strict": true,
    "strictPropertyInitialization": false
  },
  "lib": ["es2015", "ESNext.AsyncIterable"],
  "exclude": ["node_modules"]
}
